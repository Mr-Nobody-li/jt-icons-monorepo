{
  "name": "@jt-icons/icon-vue-components",
  "version": "1.1.0",
  "description": "Vue components of JT Icons collection.",
  "type": "module",
  "keywords": [
    "icon",
    "svg"
  ],
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/Mr-Nobody-li/jt-icons-monorepo/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mr-Nobody-li/jt-icons-monorepo",
    "directory": "packages/icon-vue-components"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./global": {
      "require": "./dist/global.cjs",
      "import": "./dist/global.js"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/types/*"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "pnpm build:generate && pnpm build:build && pnpm build:types",
    "build:generate": "tsx build/generate.ts",
    "build:build": "tsx build/build.ts",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@pnpm/logger": "^4.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.0.0",
    "@types/prettier": "^2.6.3",
    "camelcase": "^7.0.0",
    "esbuild": "^0.14.48",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "prettier": "^2.7.1",
    "semver": "^7.3.7",
    "tsx": "^3.6.0",
    "typescript": "^4.7.4",
    "unplugin-vue": "^2.2.1",
    "vue": "^3.2.37",
    "vue-tsc": "^0.38.2"
  }
}
